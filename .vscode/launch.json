{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dashmin.Server",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Server",
            "program": "${workspaceFolder}/Dashmin.Server/bin/Debug/netcoreapp3.1/Dashmin.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Dashmin.Server",
            "requireExactSource": false,
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
